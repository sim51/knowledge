msgstr ""

#. type: Plain text
#: site/knowledge/odbc-jdbc-bridge-neo4j/index.adoc:71
#: site/knowledge/odbc-jdbc-bridge-neo4j/index.adoc:124
msgid "| 1 | Go to the **Control Panel**"
msgstr ""

#. type: Plain text
#: site/knowledge/odbc-jdbc-bridge-neo4j/index.adoc:74
#: site/knowledge/odbc-jdbc-bridge-neo4j/index.adoc:127
msgid "| 2 | Then in **Administrative Tools**"
msgstr ""

#. type: Plain text
#: site/knowledge/odbc-jdbc-bridge-neo4j/index.adoc:77
#: site/knowledge/odbc-jdbc-bridge-neo4j/index.adoc:130
msgid "| 3 | Click on Data Sources (ODBC)"
msgstr ""

